<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet id="2" author="luan.augusto">
        <createTable tableName="TB_ANEXO">

            <column name="ID" type="NUMBER">
                <constraints nullable="false"/>
            </column>

            <column name="FILE" type="varchar(80)">
                <constraints nullable="false"/>
            </column>

            <column name="FILENAME" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <addPrimaryKey tableName="TB_ANEXO" columnNames="ID" constraintName="PK_ANEXO"/>
        <createSequence sequenceName="SQ_ANEXO" incrementBy="1" startValue="1"/>
    </changeSet>

</databaseChangeLog>
